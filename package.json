{
  "name": "raggedy",
  "version": "1.0.0",
  "private": true,
  "description": "RAG application with desktop app and server backend",
  "main": "index.js",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "test": "npm run test --workspaces",
    "build": "npm run build --workspaces",
    "dev": "npm run dev --workspaces --if-present > output.log 2>&1",
    "install:all": "npm install",
    "clean": "npm run clean --workspaces --if-present && rm -rf node_modules"
  },
  "keywords": [
    "rag",
    "desktop",
    "server",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.2.0",
    "wait-on": "^8.0.4"
  },
  "dependencies": {
  }
}
